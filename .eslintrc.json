{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "quotes": [
      "error",
      "single"
    ],
    "comma-dangle": [
      "error",
      "always-multiline"
    ],
    "eol-last": "error",
    "no-trailing-spaces": "error",
    "semi": [
      "error",
      "never"
    ],
    "no-console": "warn",
    "no-unused-vars": "error"
  }
}
